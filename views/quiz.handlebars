<header class="quiz-header">
    <div class="quiz-logo">
        <img src="images/logo/logo-w.png" class="img-quiz-logo" alt="">

    </div>

</header>


<div class="quiz-content">



    <div class="quiz-image">
        <img id="quiz_img" name="thumb-1920-943148.jpg" src="images/thumb-1920-943148.jpg" alt="">
    </div>


    <div class="quiz-right">

        <div class="select-box select-box-1 ">
            <select name="" id="quiz_visibility">
                <option value="1">Visible to </option>
                <option value="1">option 2</option>
                <option value="1">option 3</option>
                <option value="1">option 4</option>
            </select>
        </div>


        <div class="select-box select-box-2">
            <select name="" id="quiz_location">
                <option value="1">Location </option>
                <option value="1">option 2</option>
                <option value="1">option 3</option>
                <option value="1">option 4</option>
            </select>
        </div>


        <div class="select-box select-box-3">
            <select name="" id="quiz_language">
                <option value="1">Language </option>
                <option value="1">option 2</option>
                <option value="1">option 3</option>
                <option value="1">option 4</option>
            </select>
        </div>



        <input id="quiz_title" type="text" placeholder="Title" class="txt-title">

        <textarea id="quiz_desc" placeholder="Description" class="txt-description"></textarea>


    </div>
</div>

<div class="quiz-enter-button">
    <button type="button" class="btn-quiz-enter" onclick="addQuiz()">Enter</button>

</div>
<script src="/javascripts/quizjs.js"></script>